{
	"name": "petbook-project",
	"version": "1.0.0",
	"description": "final fullstack project for the masterschool web development bootcamp",
	"main": "index.js",
	"scripts": {
		"start": "tsc && node ./dist/server.js",
		"watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess \"node ./dist/server.js\"",
		"test": "db-migrate --env test up && jest --runInBand --detectOpenHandles && db-migrate --env test down -c7 --config ./database.json",
		"test-one": "db-migrate --env test up && jest src/tests/users_handlers.test.ts --runInBand --detectOpenHandles && db-migrate --env test down -c7 --config ./database.json",
		"test-db-up": "db-migrate --env test up --config ./database.json",
		"test-db-down": "db-migrate --env test down -c7 --config ./database.json",
		"migrate-up": "db-migrate up --config ./database.json",
		"migrate-down": "db-migrate down -c7 --config ./database.json",
		"tsc": "tsc",
		"db": "docker-compose up"
	},
	"keywords": [],
	"author": "Ladybug",
	"license": "ISC",
	"dependencies": {
		"bcrypt": "^5.0.1",
		"body-parser": "^1.20.0",
		"cors": "^2.8.5",
		"express": "^4.18.1",
		"jsonwebtoken": "^8.5.1",
		"pg": "^8.7.3",
		"typescript": "^4.7.4"
	},
	"devDependencies": {
		"@types/bcrypt": "^5.0.0",
		"@types/cors": "^2.8.12",
		"@types/express": "^4.17.13",
		"@types/jest": "^28.1.6",
		"@types/jsonwebtoken": "^8.5.8",
		"@types/pg": "^8.6.5",
		"@types/supertest": "^2.0.12",
		"db-migrate": "^0.11.13",
		"db-migrate-pg": "^1.2.2",
		"dotenv": "^16.0.1",
		"jest": "^28.1.3",
		"supertest": "^6.2.4",
		"ts-jest": "^28.0.7",
		"ts-node": "^10.9.1",
		"tsc-watch": "^5.0.3"
	}
}
