{
	"name": "petbook-project",
	"version": "1.0.0",
	"description": "final fullstack project for the masterschool web development bootcamp",
	"main": "index.js",
	"scripts": {
		"start-react": "react-scripts start",
		"build-react": "react-scripts build",
		"test-react": "react-scripts test",
		"eject-react": "react-scripts eject",
		"start": "tsc && node ./dist/server.js",
		"watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess \"node ./dist/server.js\"",
		"test": "db-migrate --env test up && jest --runInBand --detectOpenHandles && db-migrate --env test down -c8 --config ./database.json",
		"test-one": "db-migrate --env test up && jest src/tests/pet_model.test.ts --runInBand --detectOpenHandles && db-migrate --env test down -c8 --config ./database.json",
		"test-db-up": "db-migrate --env test up --config ./database.json",
		"test-db-down": "db-migrate --env test down -c8 --config ./database.json",
		"migrate-up": "db-migrate up --config ./database.json",
		"migrate-down": "db-migrate down -c8 --config ./database.json",
		"tsc": "tsc",
		"db": "docker-compose up"
	},
	"keywords": [],
	"author": "Ladybug",
	"license": "ISC",
	"dependencies": {
		"bcrypt": "^5.0.1",
		"body-parser": "^1.20.0",
		"cors": "^2.8.5",
		"express": "^4.18.1",
		"jsonwebtoken": "^8.5.1",
		"pg": "^8.7.3",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-redux": "^8.0.2",
		"react-router-dom": "^6.3.0",
		"react-scripts": "^5.0.1",
		"redux": "^4.2.0",
		"redux-thunk": "^2.4.1",
		"typescript": "^4.7.4"
	},
	"devDependencies": {
		"@babel/plugin-transform-modules-commonjs": "^7.18.6",
		"@babel/preset-react": "^7.18.6",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^13.3.0",
		"@types/bcrypt": "^5.0.0",
		"@types/cors": "^2.8.12",
		"@types/express": "^4.17.13",
		"@types/jest": "^28.1.6",
		"@types/jsonwebtoken": "^8.5.8",
		"@types/node": "^18.7.13",
		"@types/pg": "^8.6.5",
		"@types/react": "^18.0.17",
		"@types/react-dom": "^18.0.6",
		"@types/supertest": "^2.0.12",
		"@types/ws": "^8.5.3",
		"babel-eslint": "^10.1.0",
		"db-migrate": "^0.11.13",
		"db-migrate-pg": "^1.2.2",
		"dotenv": "^16.0.1",
		"jest": "^28.1.3",
		"supertest": "^6.2.4",
		"ts-jest": "^28.0.7",
		"ts-node": "^10.9.1",
		"tsc-watch": "^5.0.3"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
